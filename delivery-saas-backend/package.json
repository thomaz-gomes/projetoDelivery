{
  "name": "delivery-saas-backend",
  "version": "1.0.0",
  "description": "Backend do sistema delivery SAAS com iFood integration",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --ignore prisma/dev.db --ignore tmp --ignore logs --ignore dev-print-endpoint --ignore .print-agent-token --ignore .print-agent-company src/server.js",
    "dev:local": "npx prisma generate && npx prisma db push && npm run dev",
    "prisma:generate:postgres": "prisma generate --schema=prisma/schema.postgres.prisma",
    "prisma:migrate:deploy": "prisma migrate deploy --schema=prisma/schema.postgres.prisma",
    "start": "node src/server.js",
    "start:prod": "npx prisma generate --schema=prisma/schema.postgres.prisma && npx prisma migrate deploy --schema=prisma/schema.postgres.prisma && node src/server.js",
    "prisma:generate": "sh -c \"set -a && [ -f ./.env.migration ] && . ./.env.migration || true && set +a && prisma generate --schema=prisma/schema.postgres.prisma\"",
    "migrate": "prisma migrate dev --name init",
    "db:seed": "sh -c \"set -a && [ -f ./.env.migration ] && . ./.env.migration || true && set +a && node src/seed.js\"",
    "seed:manual": "node src/seed.js",
    "db:seed:large": "sh -c \"set -a && [ -f ./.env.migration ] && . ./.env.migration || true && set +a && node src/seed-large.js\"",
    "db:seed:full": "sh -c \"set -a && [ -f ./.env.migration ] && . ./.env.migration || true && set +a && npx prisma generate && npx prisma db push && node src/seed.js && node src/seed-large.js\"",
    "migrate:sqlite-to-postgres": "bash scripts/migrate_sqlite_to_postgres.sh",
    "worker:ifood": "node src/workers/ifoodPoller.js",
    "test:unit": "node --test tests/affiliates.helpers.test.mjs"
  },
  "keywords": [
    "delivery",
    "saas",
    "ifood",
    "express",
    "prisma",
    "vue"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "escpos": "^3.0.0-alpha.6",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-thermal-printer": "^4.5.0",
    "pdfkit": "^0.13.0",
    "qrcode": "^1.5.4",
    "sharp": "^0.34.5",
    "socket.io": "^4.8.1",
    "xlsx": "^0.18.5",
    "zod": "^4.1.12"
  },
  "dependenciesMeta": {
    "uuid": { "optional": true }
  },
  "optionalDependencies": {
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "ngrok": "^5.0.0-beta.2",
    "nodemon": "^3.1.10",
      "prisma": "^6.19.0"
  }
}
