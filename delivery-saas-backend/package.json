    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "escpos": "^3.0.0-alpha.6",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-thermal-printer": "^4.5.0",
    "pdfkit": "^0.13.0",
    "qrcode": "^1.5.4",
    "socket.io": "^4.8.1",
    "xlsx": "^0.18.5",
    "zod": "^4.1.12"
